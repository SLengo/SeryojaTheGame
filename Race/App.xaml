        <BitmapImage x:Key="ee_1" UriSource="easter_eggs/1.png" />
        <BitmapImage x:Key="ee_2" UriSource="easter_eggs/2.png" />
        <BitmapImage x:Key="ee_3" UriSource="easter_eggs/3.png" />
        <BitmapImage x:Key="ee_4" UriSource="easter_eggs/4.png" />
