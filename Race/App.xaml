        <Image x:Key="hat_0" Source="hats/none.png" />
        <Image x:Key="hat_1" Source="hats/hat_1.png" />
        <Image x:Key="hat_2" Source="hats/hat_2.png" />
        <Image x:Key="hat_3" Source="hats/hat_3.png" />
        <Image x:Key="hat_4" Source="hats/hat_4.png" />
