        <BitmapImage x:Key="btm_hat_0" UriSource="hats/none.png" />
        <BitmapImage x:Key="btm_hat_1" UriSource="hats/hat_1.png" />
        <BitmapImage x:Key="btm_hat_2" UriSource="hats/hat_2.png" />
        <BitmapImage x:Key="btm_hat_3" UriSource="hats/hat_3.png" />
        <BitmapImage x:Key="btm_hat_4" UriSource="hats/hat_4.png" />
